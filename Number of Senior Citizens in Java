class Solution {
    public int countSeniors(String[] details) {
        int count=0;

        for(String detail:details){
            String subStr=detail.substring(11,13);
            int ages=Integer.valueOf(subStr);
            if(ages>60){
                count++;
            }
        }
        return count;
    }
}
